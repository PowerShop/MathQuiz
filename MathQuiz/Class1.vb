Public Class Operations
    'Operation Add
    Public Function Adds(ByVal a As Integer, ByVal b As Integer) As Integer
        Return a + b
    End Function

    'Operation Subtract
    Public Function Subtracts(ByVal a As Integer, ByVal b As Integer) As Integer
        Return a - b
    End Function

    'Operation Multiply
    Public Function Multiplies(ByVal a As Integer, ByVal b As Integer) As Integer
        Return a * b
    End Function

    'Operation Divide
    Public Function Divides(ByVal a As Integer, ByVal b As Integer) As Integer
        Return a / b
    End Function

    'Operation Modulus
    Public Function Modulus(ByVal a As Integer, ByVal b As Integer) As Integer
        Return a Mod b
    End Function
End Class
